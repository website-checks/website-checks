{
  "name": "@website-checks/website-checks",
  "version": "1.20.3",
  "main": "index.js",
  "description": "checks websites with multiple services",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:check-your-website": "node index.js iana.org --check-your-website",
    "test:crtsh": "node index.js iana.org --crtsh",
    "test:cryptcheck": "node index.js iana.org --cryptcheck",
    "test:hstspreload": "node index.js iana.org --hstspreload",
    "test:httpobservatory": "node index.js iana.org --httpobservatory",
    "test:lighthouse": "node index.js iana.org --lighthouse",
    "test:pingdom": "node index.js iana.org --pingdom",
    "test:psi": "node index.js iana.org --psi",
    "test:securityheaders": "node index.js iana.org --securityheaders",
    "test:ssllabs": "node index.js iana.org --ssllabs",
    "test:webbkoll": "node index.js iana.org --webbkoll",
    "test:webhint": "node index.js iana.org --webhint",
    "test:yellowlab": "node index.js iana.org --yellowlab"
  },
  "bin": {
    "website-checks": "./bin/website-checks.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/website-checks/website-checks.git"
  },
  "keywords": [
    "certificate",
    "cryptcheck",
    "hsts",
    "hsts preload",
    "http observatory",
    "lighthouse",
    "pagespeed insights",
    "psi",
    "security headers",
    "sonarwhal",
    "ssl decoder",
    "ssllabs"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/website-checks/website-checks/issues"
  },
  "homepage": "https://github.com/website-checks/website-checks#readme",
  "dependencies": {
    "kleur": "4.0.2",
    "puppeteer": "5.3.1"
  },
  "devDependencies": {
    "hint": "^7.1.0",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=10.12.0"
  }
}
